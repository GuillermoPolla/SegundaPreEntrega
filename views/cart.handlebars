<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Cart</title>
</head>
<body>
  <h1>Carrito</h1>
  <ul>
    {{#each cart.products}}
      <li>
        <h2>{{this.product.title}}</h2>
        <p>{{this.product.description}}</p>
        <p>Precio: {{this.product.price}}</p>
        <p>Cantidad: {{this.quantity}}</p>
      </li>
    {{/each}}
  </ul>
</body>
</html>
